derived derived_add( a, b ) = a + b
derived derived_mul( a, b ) = a * b
derived derived_add2( a ) = 2 + a
derived pi = 314

rule main = {
	assert derived_add( 1, 3 ) = 4
	assert derived_mul( 1, 3 ) = 3
	assert pi = 314
	assert derived_add2( 1 ) = 3
	assert derived_add2( 0 ) = 2

	program(self) := undef
}
init main
