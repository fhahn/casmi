// transform, cmdline "--rle"

CASM RedundantLookupElimination

init foo

function bar : -> Int

rule foo = 
{
    bar := bar                  // costs: 2 -> UP + LU
    
    print bar                   // costs: 1 -> LU

    print bar                   // costs: 1 -> LU
    
    program(self) := undef
}

