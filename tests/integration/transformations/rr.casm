// transform, cmdline "--frr"

CASM rr

init foo

rule inline =
{
    debuginfo silent (1 + 1)
    debuginfo silent (2 + 2)
    debuginfo silent (3 + 3)
    debuginfo silent (4 + 4)
    debuginfo silent (5 + 5)
}

rule foo = 
{
    print (1 + 2)
    
    print (1 + 3)

    call inline

    print (2 + 4)

    print (3 + 2)

    program(self) := undef
}
