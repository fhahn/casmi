// transform, cmdline "--fcp"

CASM const_prop

init exec

rule exec =
{
    let x = 20 in       // this constant is only once in this rule scope, no propergation!
    {
        print 123       // __c0 =: 123

        print 123       

        if x = 22  then  // this if will be folded to a false -> else path will be used!
            print 123
        else
            print 123

        print 123
    
        print x
    }
    
    program(self) := undef
}

rule func(x : Int, y : Int) =
{
    print x + y
}
